#include<stdio.h>
int main()
{
	char str[1000],ch;
	int count=0,i;
	prinf("enter a string:");
	fgets(str,sizeof(str),stdin);
	printf("enter a character to find frequency:");
	scanf("%c",&ch);
    for ( i=0;str[i] !='\0';i++)
	{
		if(ch==str[i])
	    ++count;
	}
	printf("frequency of%c=%d,ch,count");
	return 0;
}
