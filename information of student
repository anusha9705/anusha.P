#include<stdio.h>
struct student
{
	char name[20];
	int roll;
	float marks;
}s;
int main()
{
	printf("enter name: ");
	fgets(s.name,sizeof(s.name),stdin);
	printf("enter roll: ");
	scanf("%d",&s.roll);
	printf("enter marks: ");
	scanf("%f",&s.marks);
	printf("name:%s",s.name);
	printf("roll:%d",s.roll);
	printf("marks:%f",s.marks);
	return 0;
