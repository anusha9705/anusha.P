#include<stdio.h>
struct value
{
	char name[50];
	int age;
}s[3];
int main()
{
	int i;
	for(i=0;i<3;++i)
	{
	printf("enter name: ");
	fgets(s[i].name,sizeof(s[i].name),stdin);
	printf("enter age: ");
	scanf("%d",&s[i].age);
	printf("name:%s\n",s[i].name);
	printf("age:%d\n",s[i].age);
    }
	return 0;
}
